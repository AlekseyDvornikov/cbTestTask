<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="settingsForm" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cb.controllers.SettingsController">
   <children>
      <TabPane layoutX="14.0" layoutY="14.0" prefHeight="374.0" prefWidth="571.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Load data">
            <content>
              <AnchorPane fx:id="settingsStage" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="applyButton" layoutX="438.0" layoutY="303.0" mnemonicParsing="false" text="Apply" />
                        <Button fx:id="cancelButton" layoutX="498.0" layoutY="303.0" mnemonicParsing="false" text="Cancel" />
                        <Button fx:id="okButton" layoutX="386.0" layoutY="303.0" mnemonicParsing="false" text="Ok" />
                        <TitledPane animated="false" layoutX="14.0" layoutY="14.0" prefHeight="89.0" prefWidth="352.0" text="File Chooser">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <Label fx:id="selectedFileLabel" cache="true" layoutX="14.0" layoutY="23.0" prefHeight="17.0" prefWidth="260.0">
                                       <opaqueInsets>
                                          <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                       </opaqueInsets>
                                    </Label>
                                    <Button fx:id="selectButton" layoutX="284.0" layoutY="19.0" mnemonicParsing="false" text="select" />
                                 </children></AnchorPane>
                          </content>
                        </TitledPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Settings">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
